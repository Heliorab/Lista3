#include <stdio.h>
#include <math.h>

int main(void) {

int numeros[3][5];
int input;
int contador = 0;
for(int i = 0; i < 3; i++) {
  for(int j = 0; j < 5; j++) {
    scanf("%d", &input);
    numeros[i][j] = input;
    //printf("%d", numeros[i][j]);
  }
}

for(int i = 0; i < 3; i++) {
  for(int j = 0; j < 5; j++) {
      if(numeros[i][j] >= 15 && numeros[i][j] <= 20 ) {
        contador += 1;
        printf("\n%d", numeros[i][j]);
      }
    
  }
}

printf("\n %d", contador);
  return 0;
}
